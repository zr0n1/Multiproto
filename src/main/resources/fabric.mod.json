{
  "schemaVersion": 1,
  "id": "multiproto",
  "version": "${version}",

  "name": "Multiproto",
  "description": "Protocol translation mod allowing Minecraft Beta 1.7.3 clients to connect to earlier Beta server versions.",
  "authors": [
    "0n1"
  ],
  "contact": {
    "homepage": "https://github.com/zr0n1/Multiproto",
    "sources": "https://github.com/zr0n1/Multiproto"
  },

  "license": "LGPLv3",
  "icon": "assets/multiproto/icon.png",

  "environment": "client",
  "entrypoints": {
    "stationapi:event_bus_client": [
      "com.github.zr0n1.multiproto.Multiproto",
      "com.github.zr0n1.multiproto.parity.TextureParityHelper"
    ],
    "gcapi": [
      "com.github.zr0n1.multiproto.Multiproto"
    ],
    "gcapi:presave": [
      "com.github.zr0n1.multiproto.Config"
    ],
    "hmifabric:item_list_refresh": [
      "com.github.zr0n1.multiproto.parity.hmifabric.HMIFabricIntegrationHelper"
    ]
  },
  "mixins": [
    "multiproto.mixins.json"
  ],

  "depends": {
    "minecraft": "1.0.0-beta.7.3",
    "station-api-base": "^${stapi_version}",
    "gcapi": "^${gcapi_version}"
  },
  "recommends": {
    "modmenu": "^${modmenu_version}"
  },
  "custom": {
    "loom:injected_interfaces": {
      "pl/telvarost/mojangfixstationapi/client/gui/multiplayer/ServerData": [
        "com/github/zr0n1/interfaces/MultiprotoServerData"
      ]
    }
  }
}
