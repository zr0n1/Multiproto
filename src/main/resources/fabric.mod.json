{
  "schemaVersion": 1,
  "id": "multiproto",
  "version": "${version}",
  "name": "Multiproto",
  "description": "Protocol translation mod allowing Minecraft Beta 1.7.3 clients to connect to earlier Beta server versions.",
  "authors": [
    "0n1"
  ],
  "contact": {
    "homepage": "https://github.com/zr0n1/Multiproto",
    "sources": "https://github.com/zr0n1/Multiproto"
  },
  "license": "LGPLv3",
  "icon": "assets/multiproto/icon.png",
  "environment": "client",
  "entrypoints": {
    "stationapi:event_bus_client": [
      {
        "adapter": "kotlin",
        "value": "com.github.zr0n1.multiproto.Multiproto"
      },
      {
        "adapter": "kotlin",
        "value": "com.github.zr0n1.multiproto.parity.optional.TextureHelper"
      }
    ],
    "gcapi": [
      {
        "adapter": "kotlin",
        "value": "com.github.zr0n1.multiproto.Multiproto"
      }
    ],
    "gcapi:presave": [
      {
        "adapter": "kotlin",
        "value": "com.github.zr0n1.multiproto.Config"
      }
    ],
    "hmifabric:item_list_refresh": [
      {
        "adapter": "kotlin",
        "value": "com.github.zr0n1.multiproto.parity.HMIFabricIntegrationHelper"
      }
    ]
  },
  "mixins": [
    "multiproto.mixins.json"
  ],
  "depends": {
    "minecraft": "1.0.0-beta.7.3",
    "fabric-language-kotlin": ">=1.11.0+kotlin.2.0.0",
    "stationapi": ">=2.0-alpha.2.1",
    "gcapi": ">=2.0.2"
  },
  "recommends": {
    "modmenu": ">=1.8.5-beta.3"
  },
  "suggests": {
    "mojangfixstationapi": ">=2.0.0",
    "hmifabric": ">=5.2.1"
  },
  "custom": {
    "loom:injected_interfaces": {
      "pl/telvarost/mojangfixstationapi/client/gui/multiplayer/ServerData": [
        "com/github/zr0n1/interfaces/MultiprotoServerData"
      ],
      "pl/telvarost/mojangfixstationapi/client/gui/multiplayer/EditServerScreen": [
        "com/github/zr0n1/interfaces/MultiprotoEditServerScreen"
      ]
    }
  }
}
